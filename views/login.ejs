<body>

  <nav class="navbar navbar-inverse navbar-fixed-top">
    <div class="container">
      <div class="navbar-header">
        <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
            <span class="sr-only">Toggle navigation</span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
            <span class="icon-bar"></span>
          </button>
        <a class="navbar-brand" href="#">UHCL Shuttle</a>
      </div>
      <div id="navbar" class="collapse navbar-collapse">
        <ul class="nav navbar-nav navbar-right">
          <li class="active"><a href="/login">Home</a></li>
          <li><a href="/about">Team</a></li>
          <li><a id="contactButton" href="#">Contact</a></li>
          <li><a href="javascript:void(0)" id="sign-in-button"><span id="account">Sign In</span></a></li>
          <li class="dropdown">
            <a href="javascript:void(0)" id="account-menu-button"></a>
          </li>
        </ul>
      </div>
