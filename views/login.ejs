  <div id="myContact" class="modal" style="text-align: left">

    <!-- Modal content -->
    <div class="modal-content">
      <div class="modal-header" style="background-color: black;border-bottom-color: black">
        <span class="closeContact">&times;</span>
        <h4 style="text-align: center; color: #e5bd2b">Contact Us</h4>
      </div>
      <div class="modal-body" style="background-color: black; border-bottom-color: black">
        <section id="contact">
          <div class="container">
            <div class="row">
              <div class="col-md-5 col-sm-5">
                <div class="form-group">
                  <label>Name: </label>
                  <input class="form-control" type="text" id="contactName" value="" placeholder="Enter Name">
                </div>
                <div class="form-group">
                  <label>Email: </label>
                  <input class="form-control" type="text" id="contactEmail" value="" placeholder="Enter Email">
                </div>
                <div class="form-group">
                  <label>Message: </label>
                  <textarea class="form-control" id="contactMessage" value="" placeholder="Enter Message" rows="10"></textarea>
                </div>
                <button id="contact-submit-button" class="btn btn-default" style="padding: 15px" onClick="contactClick()">Submit</button>
              </div>
              <div class="col-md-7 col-sm-7">

              </div>
            </div>
          </div>
        </section>
      </div>
      <!-- <div class="modal-footer">
             <h3>Modal Footer</h3>
           </div> -->
    </div>
    <!-- <h1>Jack Tran</h1> -->
  </div>

  <div class="starter-template">
    <div class="container login-form">
      <div class="row">
        <div id="login-card" class="col-md-10 col-sm-offset-1">
          <div class="panel panel-primary panel-transparent col-md-7 col-sm-offset-2">
            <div class="panel-heading">- WebApp Login -</div>
            <div class="panel-body">
              <div class="row">
                <div id="loginSection" class="col-md-12">
                  <form id="login-form" class="form-signin">

                    <div class="form-group row">
                      <div class="col-sm-12">
                        <div class="input-group">
                          <span class="input-group-addon"><i class="glyphicon glyphicon-envelope"></i></span>
                          <input type="email" class="form-control" id="username-input" name="email" placeholder="Enter uhcl.edu domain E-mail" required autofocus>
                        </div>
                      </div>
                    </div>

                    <div class="form-group row">
                      <div class="col-sm-12">
                        <div class="input-group" id="password-input-mdl-textfield">
                          <span class="input-group-addon"><i class="glyphicon glyphicon-lock"></i></span>
                          <input type="password" class="form-control" id="password-input" name="password" placeholder="Enter Password" required>
                        </div>
                      </div>
                    </div>

                    <div class="row">
                      <div id="login-section-buttons" class="col-sm-12 ">
                        <button id="login-button" type="button" class="btn btn-primary">LOGIN</button>
                        <button id="register-button" type="button" class="btn btn-primary">SIGN UP</button>
                      </div>
                    </div>

                  </form>

                </div>
              </div>
              <div class="center">- or -</div>
              <div class="row">
                <div id="socialLoginSection" class="col-sm-12">
                  <button id="facebook-button" type="button" class="btn btn-facebook oauth-login-button"><i class="fa fa-facebook"></i>Facebook</button>
                  <button id="google-button" type="button" class="btn btn-google oauth-login-button"><i class="fa fa-google-plus"></i>Google</button>
                </div>
              </div>

            </div>
          </div>
          <!-- End of Panel panel-primary panel-transparent -->
        </div>
        <!--End of Login-card -->
      </div>
      <div id="register-card">
        <!-- Start of register-card -->
        <div class="row">
          <div class="col-md-6 col-md-offset-3">
            <div class="row">
              <div class="card col-md-12" style="margin:20px;">
                <div class="row">
                  <div id="card-header" class="card-header text-center col-md-12">- Register -</div>
                </div>

                <div class="card-block">
                  <div class="row card-body">
                    <form id="formRegister" class="col-md-12">

                      <input type="email" id="email" class="form-control transparent-input" name="email" placeholder="Enter uhcl.edu domain E-mail" required autofocus>
                      <input type="text" id="display-name" class="form-control transparent-input" name="displayName" placeholder="Enter name to be displayed" required>
                      <input type="tel" id="phone" class="form-control transparent-input" name="phone" placeholder="Enter valid telephone number" required>
                      <input type="password" id="registration-input-password" class="form-control transparent-input" name="registrationPassword" placeholder="Password" required>
                      <div id="registration-input-password2-mdl-textfield">
                        <input type="password" id="registration-input-password2" class="form-control transparent-input" name="registrationPassword2" placeholder="Re-enter password" required>
                      </div>
                    </form>
                  </div>
                </div>
                <div class="row card-body">
                  <div class="card-footer text-muted col-md-12">
                    <div class="row">
                      <div id="registerButtons" class="col-md-12">
                        <a id="back-button" href="#" class="btn btn-primary">Back</a>
                        <a id="submit-button" class="btn btn-primary">Submit</a>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
      <!-- End of Register-card -->
      <div id="notice-card">
        <!-- Start of notice-card -->
        <div class="row">
          <div class="col-md-6 col-md-offset-3">
            <div class="row">
              <div class="card col-md-12" style="margin:20px;">
                <div class="row">
                  <div id="card-header" class="card-header text-center col-md-12">- Notice -</div>
                </div>
                <div class="card-block">
                  <div class="row card-body">
                    <div id="register-supporting-text">
                      <h6 id="notice-card-h6">Check your email address for your verification code.</h6>
                    </div>
                  </div>
                </div>
                <div class="row card-body">
                  <div class="card-footer text-muted col-md-12">
                    <div class="row">
                      <div id="registerButtons" class="col-md-12">
                        <a id="exit-button" class="btn btn-primary">Exit</a>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
        <!-- end of row div -->
      </div>
      <!-- End of notice-card -->
      <div id="logout-card">
        <!-- Start of logout-card -->
        <div class="row">
          <div class="col-md-6 col-md-offset-3">
            <div class="row">
              <div class="card col-md-12" style="margin:20px;">
                <div class="row">
                  <div id="card-header" class="card-header text-center col-md-12">- Signed In -</div>
                </div>
                <div class="card-block">
                  <div class="row card-body">
                    <div id="register-supporting-text">
                      <h6 id="logout-card-h6">You are now signed in. To sign out or update your account click on your display name in the top right corner.</h6>
                    </div>
                  </div>
                </div>
                <div class="row card-body">
                  <div class="card-footer text-muted col-md-12">
                    <div class="row">
                      <div id="registerButtons" class="col-md-12">
                        <a id="home-Button" href="/" class="btn btn-primary">Home</a>
                      </div>
                    </div>
                  </div>
                </div>
              </div>
            </div>
          </div>
        </div>
      </div>
      <!-- End of logout-card -->

      <!--Toast Messages Markup -->
      <div id="toast" class="mdl-js-snackbar mdl-snackbar">
        <div class="mdl-snackbar__text"></div>
        <button class="mdl-snackbar__action" type="button"></button>
      </div>
      <!-- The actual Snackbar
           <div id="alert-snackbar" class=" alert animated fadeInUp" style="display: none;">
            </div>-->

      <!-- Snackbar ends -->

    </div>
    <!-- End of  Container Login Form -->


  </div>
  <!-- End of Starter-template div -->


  <section class="footer">
    <div class="container">
      <div class="col-sm-6 col-md-6">
        <p class="text-muted" style="text-align: left"> 2017 University of Houston - Clear-Lake</p>
      </div>
      <div class="col-sm-6 col-md-6">
        <p class="text-muted" style="text-align: right">2700 Bay Area Boulevard, Houston, TX 77058</p>
      </div>
    </div>
  </section>



